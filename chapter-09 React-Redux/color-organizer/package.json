{
  "name": "color-organizer",
  "version": "4.0.0",
  "description": "A sample React application",
  "private": true,
  "scripts": {
    "build-css": "node-sass stylesheets/scss -o stylesheets",
    "watch-css": "npm run build-css && node-sass stylesheets/scss -o stylesheets --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "React",
    "state",
    "setState",
    "explicitly",
    "passing",
    "props"
  ],
  "author": "Alex Banks <alex@moonhighway.com> (http://www.moonhighway.com)",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.4",
    "redux": "3.6.0",
    "uuid": "3.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
